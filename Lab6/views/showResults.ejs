<html lang="en-US">
<head>
    <title>Survey Analysis</title>
</head>

<body>
    <h1>Survey analysis</h1>
    <h2>Gender</h2>
    <ul>
        <% for (var i=0; i<results[0].length; i++){ %>
            <li><%= results[0][i].gender %> :  <%= results[0][i].count %></li>
        <% } %>    
        </ul>
    <h2>Colors</h2>
    <ul>
    <% for (var i=0; i<results[1].length; i++){ %>
        <li><%= results[1][i].color %> :  <%= results[1][i].count %></li>
    <% } %>    
    </ul>
    <h2>Age</h2>
    <ul>
    <% for (var i=0; i<results[2].length; i++){ %>
        <li><%= results[2][i].age %> :  <%= results[2][i].count %></li>
    <% } %>    
    </ul>
    <h2>Favourite aspect</h2>
    <ul>
    <% for (var i=0; i<results[3].length; i++){ %>
        <li><%= results[3][i].fav %> :  <%= results[3][i].count %></li>
    <% } %>    
    </ul>
    <h2>Aspects can be changed</h2>
    <ul>
    <% for (var i=0; i<results[4].length; i++){ %>
        <li><%= results[4][i].aspect %> :  <%= results[4][i].count %></li>
    <% } %>    
    </ul>

    <h2>Comments</h2>
    <ul>
    <% for (var i=0; i<results[5].length; i++){ %>
        <li><%= results[5][i].comment %></li>
    <% } %>    
    </ul>
    
</body>
</html>